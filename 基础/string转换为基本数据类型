 * @author hongyi
 * @version 1.0
 */


package text;


public class Text 
{ 
	 public static void main(String[] args)
	    {
		 String str1 = "123";
		 System.out.println(getType(str1));
		 int num = Integer.parseInt(str1);
		 System.out.println(getType(num));//改变原有的数据类型
		 System.out.println(getType(str1));//不改变原有的数据类型
		 double num2 = Double.parseDouble(str1);
		 System.out.println(getType(num2));//改变原有的数据类型
		 System.out.println(getType(str1));//不改变原有的数据类型
	    }


	 public static String getType(Object i) {	
	 	return i.getClass().toString();
	 }
}
